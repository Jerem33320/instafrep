{% extends 'base.html.twig' %}

{% block metaDescription %}
Donnez votre age et on vous dira quand vous êtes né !
{% endblock %}

{% block pageContent %}

    {{ parent() }}

    {% include 'navbar.html.twig' %}


    {% if year is not empty %}
        Vous êtes né en : {{ year }}
